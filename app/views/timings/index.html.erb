<div id="errors"></div>
<div class="card">
  <div class="card-header text-center">
    <h5><strong>
      <%= @screen.theatre.name %>
    </strong></h5>
    <h5><%= @screen.name %></h5>
  </div>
  <div class="card-body">
    <div class="card-columns" id="timings">
      <%= render @screen.timings %>
    </div>
    <div id="timing-form"></div>
  </div>
  <div class="card-footer">
    <%= link_to "Add Timing", new_timing_path(screen_id: @screen.id), remote: true, class: "btn btn-secondary" %>
  </div>
</div>
